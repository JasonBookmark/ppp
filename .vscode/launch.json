{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "WebAssembly benchmarks",
            "program": "${workspaceFolder}/webapp/benchmark.js"
        },
        {
            "name": "PPP UT (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "Build Debug",
            "program": "build_windows_debug_x64/libppp/ppp_test.exe",
            "stopAtEntry": false,
            "externalConsole": false,
            "logging": {
                "traceResponse": true,
                "engineLogging": true,
                "trace": true
            }
        },
        {
            "name": "Python Build Script",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${workspaceFolder}/build.py",
            "args": [
                "--emscripten",
                "--build_config=release"
            ]
        }
    ]
}